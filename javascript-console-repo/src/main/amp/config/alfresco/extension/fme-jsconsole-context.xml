<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>

	<bean id="webscript.de.fme.jsconsole.execute.post" class="de.fme.jsconsole.ExecuteWebscript" parent="webscript">
		<property name="scriptUtils" ref="utilsScript"/>
		<property name="transactionService" ref="transactionService"/>
		<property name="preRollScriptResource" value="de/fme/jsconsole/jsconsole-pre-roll-script.js" />
		<property name="postRollScriptResource" value="de/fme/jsconsole/jsconsole-post-roll-script.js" />
	</bean>

    <bean id="webscript.de.fme.jsconsole.oauth-token.get" class="de.fme.jsconsole.GetOAuthToken" parent="webscript">
        <property name="oauth2CredentialsStoreService">
            <ref bean="oauth2CredentialsStoreService" />
        </property>
    </bean>
    
    <bean id="webscript.de.fme.jsconsole.oauth-token.post" class="de.fme.jsconsole.SaveOAuthToken" parent="webscript">
        <property name="oauth2CredentialsStoreService">
            <ref bean="oauth2CredentialsStoreService" />
        </property>
    </bean>
    
</beans>

