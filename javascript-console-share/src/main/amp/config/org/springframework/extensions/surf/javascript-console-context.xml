<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
   
   <!-- Add Admin Console messages -->
   <bean id="share-extras.javascript-console.resources" class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
      <property name="resourceBundles">
         <list>
            <value>alfresco.messages.javascript-console</value>
         </list>
      </property>
   </bean>
   
    <bean id="webscript.de.fme.jsconsole.github-oauth2-return.get" parent="webscript.org.sharextras.slingshot.oauth2-return.get">
        <property name="endpointId" value="github-oauth" />
        <property name="clientId" value="478fcdc445fa7b29ecf3" />
        <property name="clientSecret" value="187d2f9181448e4b0b260125a39aa4d04133fe49" />
        <property name="accessTokenUrl" value ="https://github.com/login/oauth/access_token" />
    </bean>
           
   <bean id="fme.evaluator.doclib.action.isAdmin" class="de.fme.jsconsole.IsAdminEvaluator" />
   
</beans>